{
  "name": "service-backend",
  "version": "1.0.0",
  "author": "Vincent Leclerc",
  "description": "Service backend",
  "keywords": [
    "service-backend"
  ],
  "os": [
    "!win32"
  ],
  "arch": "x64",
  "engines": {
    "node": ">=16.18.0",
    "npm": ">=8.19.2",
    "mongodb": ">=4.4"
  },
  "endpoints": {
    "v1": "1.0"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:vincent-leclerc-dev/service-backend.git"
  },
  "scripts": {
    "init:commit-message": "npx commitizen init cz-conventional-changelog --save-dev --save-exact",
    "init:hooks": "husky install",
    "init": "npm run init:hooks && npm run init:commit-message",
    "lint": "eslint /src",
    "lint:fix": "eslint /src --fix",
    "test": "NODE_ENV=development jest --config ./test/unit/jest-unit.js --collectCoverage --reporters=default --reporters=jest-junit",
    "test:dev": "NODE_ENV=development jest --config ./test/unit/jest-unit.js",
    "test:watch": "NODE_ENV=development jest --watch --config ./test/unit/jest-unit.js",
    "test:cov": "NODE_ENV=development jest --coverage --config ./test/unit/jest-unit.js",
    "test:debug": "NODE_ENV=development node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "NODE_ENV=development npx cucumber-js --tags 'not @skip'",
    "prebuild": "rimraf dist",
    "build": "nest build",
    "build:docker": "docker build .",
    "start": "nest start api-gateway",
    "start:dev": "NODE_ENV=development nest start --watch api-gateway",
    "start:debug": "DEBUG=true NODE_ENV=development nest start --debug --watch api-gateway",
    "start:prod": "NODE_ENV=production node dist/main",
    "start-consumer:dev": "NODE_ENV=development nest start --watch service-consumer",
    "start-consumer:debug": "DEBUG=true NODE_ENV=development nest start --watch service-consumer",
    "start-consumer:prod": "NODE_ENV=production node dist/main"
  },
  "dependencies": {
    "@fastify/compress": "6.1.0",
    "@fastify/helmet": "9.1.0",
    "@fastify/static": "^6.5.0",
    "@nestjs/bull": "^0.6.1",
    "@nestjs/common": "9.0.8",
    "@nestjs/config": "2.2.0",
    "@nestjs/core": "9.0.8",
    "@nestjs/mapped-types": "1.1.0",
    "@nestjs/mongoose": "^9.2.0",
    "@nestjs/platform-fastify": "9.1.6",
    "@nestjs/swagger": "^6.1.3",
    "@nestjs/typeorm": "^9.0.0",
    "ajv": "6.12.6",
    "bluebird": "^3.7.2",
    "bull": "^4.10.1",
    "class-transformer": "0.5.1",
    "class-validator": "0.13.2",
    "compression": "1.7.4",
    "consis": "1.3.2",
    "dotenv": "^16.0.1",
    "helmet": "5.0.1",
    "js-convert-case": "4.2.0",
    "lodash": "^4.17.21",
    "mailchecker": "^5.0.3",
    "mongoose": "^6.7.0",
    "nestjs-pino": "3.1.1",
    "pino-http": "6.6.0",
    "reflect-metadata": "0.1.13",
    "rimraf": "3.0.2",
    "rxjs": "7.2.0",
    "typeorm": "^0.3.10"
  },
  "devDependencies": {
    "@cucumber/cucumber": "8.2.0",
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^9.0.0",
    "@types/bluebird": "^3.5.37",
    "@types/bull": "^4.10.0",
    "@types/express": "^4.17.13",
    "@types/jest": "28.1.8",
    "@types/node": "^16.0.0",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "commitizen": "^4.2.5",
    "cucumber-tsflow": "4.0.0-preview.7",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "8.6.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-airbnb-typescript": "16.1.4",
    "eslint-import-resolver-typescript": "2.7.1",
    "eslint-plugin-import": "2.25.4",
    "husky": "8.0.1",
    "jest": "28.1.3",
    "jest-junit": "13.2.0",
    "module-alias": "2.2.2",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "28.0.8",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "4.1.0",
    "typescript": "^4.7.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/apps/"
    ]
  },
  "private": true,
  "preferGlobal": false,
  "analyze": false,
  "license": "UNLICENSED",
  "_moduleAliases": {
    "@": "src"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
